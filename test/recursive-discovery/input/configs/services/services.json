{
  "services": {
    "auth": {
      "enabled": true,
      "provider": "oauth2",
      "timeout": 10
    },
    "cache": {
      "enabled": true,
      "provider": "redis",
      "ttl": 3600
    },
    "monitoring": {
      "enabled": true,
      "provider": "prometheus",
      "interval": 30
    }
  }
}
